    "url": "https://github.com/AksRK/rafl-media-backend/issues"
  },
  "homepage": "https://github.com/AksRK/rafl-media-backend#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "concurrently": "^7.5.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.3",
    "mongoose-paginate-v2": "^1.7.1",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.20",
    "uuid": "^9.0.0"
